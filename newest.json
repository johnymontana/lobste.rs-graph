ents_url":"https://lobste.rs/s/bv2nje/tag_suggestion_nix","submitter_user":{"username":"fs111","created_at":"2017-01-04T07:51:28.000-06:00","is_admin":false,"about":"I am a software engineer from Berlin ","is_moderator":false,"karma":3554,"avatar_url":"/avatars/fs111-100.png","invited_by_user":"drdoom","github_username":"fs111","twitter_username":"fs111"},"tags":["meta"]}]